  "scripts": {
    "lint": "npx eslint ./src --ext .js,.jsx,.ts,.tsx",
    "doc": "rimraf docs && jsdoc -r  . -d docs -c jsdoc.conf README.md",
    "cypress": "npx cypress open",
    "test": "npx cypress run",
    "removebuild": "rimraf build",
    "webpack": "webpack",
    "webpack-dev-server": "webpack-dev-server",
    "livedev": "webpack-dev-server --mode=development --host 0.0.0.0",
    "dev": "env-cmd -e development webpack --mode=development",
    "prod": " env-cmd -e production webpack --mode=production",
    "staging": " env-cmd -e staging webpack --mode=production",
    "firebase": "firebase emulators:start --only firestore,functions --export-on-exit ./data --import ./data",
    "firebase:local": "npm run functions:config:local && env-cmd -e development firebase emulators:start --export-on-exit ./data --import ./data -P staging",
    "firebase:stage": "npm run functions:config:stage && firebase functions:config:get > functions/.runtimeconfig.json && env-cmd -e staging firebase emulators:start --only functions -P staging",
    "functions:config:local": "firebase functions:config:set env.bq_project=chalk-dev-c6a5d env.bq_dataset=observations_dev && firebase functions:config:get > functions/.runtimeconfig.json",
    "functions:config:stage": "firebase functions:config:set env.bq_project=chalk-dev-c6a5d env.bq_dataset=observations",
    "localdev": "env-cmd -e development webpack-dev-server --mode=development",
    "deploydocs": "npm run doc && firebase target:apply hosting chalkdocs chalkdocs && firebase deploy --only hosting:chalkdocs",
    "deploy": "npm run removebuild && firebase target:apply hosting cqrefpwa cqrefpwa && npm run prod && firebase deploy --only hosting:cqrefpwa",
    "deploy:preview": "npm run removebuild && npm run prod && firebase target:apply hosting cqrefpwa cqrefpw && firebase hosting:channel:deploy prod_preview",
    "deploy:stage": "npm run removebuild && npm run functions:config:stage && npm run staging && firebase deploy -P staging",
    "devdeploy": "npm run removebuild && firebase target:apply hosting devcqref devcqref && npm run dev && firebase deploy --only hosting:devcqref",
    "stagingdeploy": "npm run removebuild && firebase target:apply hosting stagingcqref stagingcqref && npm run prod && firebase deploy --only hosting:stagingcqref"
  },
